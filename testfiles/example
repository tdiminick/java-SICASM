EXAM      START   100
EXA       LDT     #14
          ADDR    T,A
          ADD     SEC
          STA     ANS,X
          BASE    BIG
          LDB     #BIG
          LDS     @ONE
          +STA     B
          LDT     =X'1AB9'
          LDS     =C'EON'
          LDA     #EXA
          STA      NUM
          J       @NUM
          RSUB        
SEC       WORD    -11 
          LTORG           
NUM       RESW    1   
ANS       RESW    2
ONE       WORD    13   
BIG       RESW    1000 
B         RESW    1    
          END     EXA